{
  "name": "feedback-loop",
  "version": "0.1.0",
  "private": true,
  "description": "Inline bidirectional feedback between developers and AI agents",
  "scripts": {
    "test": "npm run test:cli && npm run test:extension",
    "test:cli": "node --test test/cli/*.test.js",
    "test:extension": "npm run compile && node --test test/extension/*.test.js",
    "test:extension:host": "cd extension && npm run test:host",
    "test:full": "npm test && npm run test:extension:host",
    "compile": "cd extension && npm run compile"
  }
}
